double[6] doubleArray; //声明实数数组
int[2]  intArray ; //声明整形数组
double a = 2.0;
int b = 0;

//给数组赋值
doubleArray[b] = 2;
doubleArray[1] = a;
doubleArray[2] = intArray[0];
a = 0.9;
doubleArray[3] = a;
doubleArray[4] = 5 - 2 / (4.0 - 3) * 2.5 + 0.01; 


read(doubleArray[5]);


//打印数组
int x = 0;
while(x < 6)
{
    write(doubleArray[x]);
    x = x + 1;
}

