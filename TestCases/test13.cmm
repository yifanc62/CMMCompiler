//选择排序
int n = 5;
int [5]a;
a[0] = 4;
a[1] = 2;
a[2] = 5;
a[3] = 3;
a[4] = 1;



 int i = 0;  
 int min = 0;  
 int j = 0;  
 int tmp = 0;  

 for(i = 0;i < n-1;i=i+1)  
 {  
    min = i; 
    for(j = i;j < n;j=j+1)  
    {  
      if(a[min]>a[j])  
      {  
       min = j;  
      }  
    }  
    if(min != i)
    {  
      tmp = a[min];  
      a[min] = a[i];  
      a[i] = tmp;  
    }  
 }  
write(a[0]);
write(a[1]);
write(a[2]);
write(a[3]);
write(a[4]);